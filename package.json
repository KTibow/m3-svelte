{
  "name": "m3-svelte",
  "version": "4.0.0",
  "license": "Apache-2.0 OR GPL-3.0-only",
  "repository": "KTibow/m3-svelte",
  "author": {
    "name": "KTibow"
  },
  "homepage": "https://github.com/KTibow/m3-svelte",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package -o package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && eslint",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "files": [
    "package",
    "!package/**/*.test.*",
    "!package/**/*.spec.*"
  ],
  "svelte": "./package/index.js",
  "types": "./package/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./package/index.d.ts",
      "svelte": "./package/index.js"
    },
    "./misc/styles.css": {
      "import": "./package/misc/styles.css"
    },
    "./misc/recommended-styles.css": {
      "import": "./package/misc/recommended-styles.css"
    }
  },
  "dependencies": {
    "@iconify/types": "^2.0.0",
    "@ktibow/iconset-material-symbols": "^0.0.1747426012",
    "@material/material-color-utilities": "^0.3.0",
    "svelte": "^5.33.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.27.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.21.1",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.9.0",
    "fast-glob": "^3.3.3",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.12",
    "svelte-check": "^4.2.1",
    "svelte-highlight": "^7.8.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5"
  },
  "keywords": [
    "svelte",
    "material",
    "material 3",
    "material you",
    "m3",
    "ui",
    "frontend",
    "design-system",
    "ui-library",
    "theming"
  ],
  "sideEffects": false,
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
