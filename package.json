{
  "name": "m3-svelte",
  "version": "7.0.0",
  "license": "Apache-2.0 OR GPL-3.0-only",
  "repository": "KTibow/m3-svelte",
  "author": {
    "name": "KTibow"
  },
  "homepage": "https://github.com/KTibow/m3-svelte",
  "funding": "https://github.com/sponsors/KTibow",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package -o package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && eslint",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "files": [
    "package",
    "!package/**/*.test.*",
    "!package/**/*.spec.*"
  ],
  "svelte": "./package/index.js",
  "types": "./package/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./package/index.d.ts",
      "svelte": "./package/index.js"
    },
    "./etc/styles.css": {
      "style": "./package/etc/styles.css"
    },
    "./etc/recommended-styles.css": {
      "style": "./package/etc/recommended-styles.css"
    },
    "./etc/tailwind-styles.css": {
      "style": "./package/etc/tailwind-styles.css"
    },
    "./etc/unocss-preset": {
      "types": "./package/etc/unocss-preset.d.ts",
      "import": "./package/etc/unocss-preset.js"
    },
    "./etc/colors": {
      "types": "./package/etc/colors.d.ts",
      "import": "./package/etc/colors.js"
    },
    "./etc/layer": {
      "types": "./package/etc/layer.d.ts",
      "import": "./package/etc/layer.js"
    }
  },
  "dependencies": {
    "@iconify/types": "^2.0.0",
    "@ktibow/iconset-material-symbols": "~0.0.1764827375"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@ktibow/material-color-utilities-nightly": "^0.3.11763580498000",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.1",
    "globals": "^16.5.0",
    "m3-svelte": "link:",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "publint": "^0.3.16",
    "shiki": "^3.20.0",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "tinyglobby": "^0.2.15",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "unocss": "^66.5.12",
    "vite": "^7.3.0",
    "vite-plugin-functions-mixins": "^0.4.0",
    "vite-plugin-token-shaker": "^0.0.3"
  },
  "peerDependencies": {
    "@ktibow/material-color-utilities-nightly": "^0.3.11763580498000",
    "svelte": "^5.40.0"
  },
  "peerDependenciesMeta": {
    "@ktibow/material-color-utilities-nightly": {
      "optional": true
    }
  },
  "keywords": [
    "svelte",
    "material",
    "material 3",
    "material you",
    "m3",
    "ui",
    "frontend",
    "design-system",
    "ui-library",
    "theming"
  ],
  "sideEffects": [
    "./package/etc/layer.js"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
